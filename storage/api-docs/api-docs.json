                    "Helpdesk"
                ],
                "summary": "Create new ticket",
                "operationId": "61f8089cf49f3e61340dc5af6dd6eb98",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "title": {
                                        "type": "string"
                                    },
                                    "content": {
                                        "type": "string"
                                    },
                                    "user_id": {
                                        "type": "integer"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Ticket created"
                    }
                }
            }
        },
        "/api/helpdesk/{id}": {
            "get": {
                "tags": [
                    "Helpdesk"
                ],
                "summary": "Get ticket details",
                "operationId": "445ea29ea600464d0d41cd9463b6a35e",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Ticket details"
                    }
                }
            }
        },
