            "post": {
                "tags": [
                    "Helpdesk"
                ],
                "summary": "Add reply to ticket",
                "operationId": "1c921dba873f4e8d86077b5030fba55e",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "required": true
                    }
